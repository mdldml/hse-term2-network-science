   "source": [
    "# Put your code here"
    "x_min = min(degrees)\n",
    "alpha = 1 + n / sum([log(xi / x_min) for xi in degrees])\n",
    "\n",
    "sigma = (alpha - 1) / sqrt(n)\n",
    "print(\"alpha: %f, sigma^2: %f\" % (alpha, sigma ** 2))"
   ]
